<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="DavidGarciaCatResqueBundle_homepage" path="/">
        <default key="_controller">DavidGarciaCatResqueBundle:Default:index</default>
    </route>

    <route id="DavidGarciaCatResqueBundle_queue_show" path="/queue/{queue}">
        <default key="_controller">DavidGarciaCatResqueBundle:Default:showQueue</default>
    </route>

    <route id="DavidGarciaCatResqueBundle_failed_list" path="/failed/list">
        <default key="_controller">DavidGarciaCatResqueBundle:Default:listFailed</default>
    </route>

    <route id="DavidGarciaCatResqueBundle_scheduled_list" path="/scheduled/list">
        <default key="_controller">DavidGarciaCatResqueBundle:Default:listScheduled</default>
    </route>

    <route id="DavidGarciaCatResqueBundle_scheduled_timestamp" path="/scheduled/{timestamp}">
        <default key="_controller">DavidGarciaCatResqueBundle:Default:showTimestamp</default>
    </route>
</routes>
