{
    "name": "david-garcia/resque-bundle",
    "type": "symfony-bundle",
    "description": "Fork with some updates of unmaintained project michelsalib/BCCResqueBundle",
    "keywords": ["resque job queue management"],
    "homepage": "http://github.com/michelsalib/BCCResqueBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Salib",
            "email": "michelsalib@hotmail.com",
            "homepage": "http://michelsalib.com"
        },
        {
            "name": "David Garcia",
            "email": "me@davidgarcia.cat"
        }
    ],
    "require": {
        "php": ">=5.5,<8",
        "symfony/framework-bundle": ">=2.0,<5",
        "david-garcia/php-resque": "^1.2",
        "david-garcia/php-resque-scheduler": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8"
    },
    "autoload": {
        "psr-4": {
            "BCC\\ResqueBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\BCC\\ResqueBundle\\": "tests"
        }
    }
}
